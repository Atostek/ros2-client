{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/target/**": true
    },
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "statiolake.vscode-rustfmt",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active"
}